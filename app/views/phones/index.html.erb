<p id="notice"><%= notice %></p>

<h1><%= "#{t('message.listing')} #{t('activerecord.models.phone.other')}" %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.phone.phone') %></th>
      <th><%= t('activerecord.attributes.phone.contact_id') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phones.each do |phone| %>
      <tr>
        <td><%= phone.phone %></td>
        <td><%= phone.contact.name %></td>
        <td><%= link_to t('buttons.show'), phone %></td>
        <td><%= link_to t('buttons.edit'), edit_phone_path(phone) %></td>
        <td><%= link_to t('buttons.destroy'), phone, method: :delete, data: { confirm: t('message.confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

      <li><%= link_to t('activerecord.models.kind.other'), kinds_path %></li>
      <li><%= link_to t('activerecord.models.phone.other'), phones_path %></li>
      <li><%= link_to t('activerecord.models.address.other'), addresses_path %></li>
    </ul>
    <%= yield %>
  </body>
